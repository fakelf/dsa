        <activity
            android:name=".Menu"
            android:label="@string/title_activity_menu" >
        </activity>
        <activity
            android:name=".Men"
            android:label="@string/title_activity_men" >
        </activity>
        <activity
            android:name=".Sanguo"
            android:label="@string/title_activity_sanguo" >
        </activity>
    </application>

</manifest>
